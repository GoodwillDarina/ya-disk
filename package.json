{
  "name": "tes-disk",
  "version": "1.0.0",
  "description": "Test application on React+Redux whit output news for authorisation users",
  "scripts": {
    "start": "set DEBUG=true && node server.js",
    "start:unix": "DEBUG=true node server.js",
