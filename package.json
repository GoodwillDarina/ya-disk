    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.22.0",
    "es6-promise": "^4.1.0",
    "file-loader": "^0.8.5",
    "image-webpack-loader": "^2.0.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "react-hot-loader": "^1.3.0",
    "react-router-dom": "^4.1.1",
